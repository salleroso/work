package ClassSkripts;

import javafx.fxml.FXML;
import javafx.scene.control.ListView;

import static com.codeborne.selenide.Selenide.$;
import static com.codeborne.selenide.Selenide.open;

public class ALogintest  {


    @FXML
    private ListView listView;
    @FXML
    private static String golfTextArea;
    @FXML
    public static String TextArea;






    public static   void LoginTest() {
        if ( TextArea == "Unitest2")
        {

            open("http://develop2app.south.rt.ru/TEST2");
            $("#login-field").val("GontarV");
            $("#pwd-field").val("020390Sider!");
            $("#signin-btn").click();

        }
        else if (TextArea == "S1uninew")
        {
            open("https://tz-uniapp.south.rt.ru/S1UNIAPP");

            $("#login-field").val("GontarV");
            $("#pwd-field").val("020390Sider!");
            $("#signin-btn").click();
        }

    }
}
